server {
    listen        80;
    server_name   clqmobile.com www.clqmobile.com;

    access_log /var/log/nginx/kliqmobile.com/clq-access.log;    
    error_log  /var/log/nginx/kliqmobile.com/clq-error.log;

    root /home/ubuntu/clqmobile.com/htdocs;
  
    index index.php index.html index.htm;
}
